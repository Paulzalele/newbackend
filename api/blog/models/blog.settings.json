{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "meta": {
      "type": "string"
    },
    "blog_date": {
      "type": "date"
    },
    "read_time": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "video": {
      "type": "component",
      "repeatable": false,
      "component": "video.video"
    },
    "slug": {
      "type": "string"
    },
    "pin": {
      "type": "boolean"
    },
    "published": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "draft"
    }
  }
}
